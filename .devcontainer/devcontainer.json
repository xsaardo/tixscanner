{
  "name": "TixScanner",
  "image": "mcr.microsoft.com/devcontainers/python:3.11",
  "features": {
    "ghcr.io/devcontainers-contrib/features/apt-packages:1": {
      "packages": [
        "wget",
        "gnupg",
        "software-properties-common"
      ]
    }
  },
  "postCreateCommand": ".devcontainer/setup.sh",
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-python.python",
        "ms-python.pylint"
      ]
    }
  },
  "forwardPorts": [],
  "remoteUser": "vscode"
}